server {
    listen       80;
    server_name  localhost;

    access_log  {{ webapps_dir }}/{{ app_name }}/logs/nginx-access.log;
    error_log {{ webapps_dir }}/{{ app_name }}/logs/nginx-error.log;

    location / {
        include uwsgi_params;
        uwsgi_pass unix:/tmp/{{ app_name }}.sock;
    }

}